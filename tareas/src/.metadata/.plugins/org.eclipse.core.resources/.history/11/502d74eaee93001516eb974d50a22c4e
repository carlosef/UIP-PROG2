				System.out.println("\ng)VOLLEYBALL ");
				System.out.println("\nh)KARATE ");
				System.out.println("\nh)flat");
				System.out.println("\nh)ping pon");
				opc = Integer.parseInt(k.readLine());
				case 2 :
				
				int iteracion=1;
				int t ;
				
					while (iteracion>10){
						++iteracion;
						for(t=0;t<=10;t++){
							
						}
						
						
						opc = Integer.parseInt(k.readLine());
					}
					
					
				
				}
	}
