				System.out.println("\n10)ping pon");
				opc = Integer.parseInt(k.readLine());
				
					break ;
				
				case 2 :
					System.out.print("\nEL TOTAL DE ENCUESTADOS ES:");
					System.out.println(" \n \n1)TENIS");
					System.out.print(" \n \n1)TENIS");
