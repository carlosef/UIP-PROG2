					System.out.println("\nh)KARATE ");
					System.out.println("\ni)PING PON ");
					System.out.println("\nj)FLAG  ");
					System.out.println("\nk)OTROS  ");
					
					
					int encuestados = 0;
					if(encuestados>=1)encuestados++;
					
					break;
				}
					
				
					
					
					
					
					
							}
				
					
					
					
					
					
					
					
					
					

