							deporte1[7] = "Petra";
							deporte1[8] = "Petra";
							deporte1[9] = "Petra";
							
				
				
					break;
				case 2:
					System.out.print("ANALISI DE LA ENCUESTA:");
					
				
					
				
				
					
					
					
					
					
					
					
					
					
					break;
					}

	}

	public static int getDeporte() {
		return deporte;
	}

	public static void setDeporte(int deporte) {
		Principal.deporte = deporte;
	}

	public static int getA() {
		return a;
	}

	public static void setA(int a) {
		Principal.a = a;
	}
}
	
				
					
				
					
					
					
					
					
							
				
					
					
					
					
					
					
					
					
					

				
				
			
					




	


					
						
					
					
	
					
					
				
	







			 		
		
		         
				 
				 			
					
					

							
				 
				  
				  
			
			
							
	




	


			
			
						
		
	


