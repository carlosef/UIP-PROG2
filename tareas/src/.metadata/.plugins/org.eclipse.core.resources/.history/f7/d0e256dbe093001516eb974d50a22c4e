import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Principal {
	
	
				
	

	

	private static String[] encustados;

	public static void main(String[] args) throws NumberFormatException, IOException {
		int opc;
		
		
		System.out.println("MENU");
			
			System.out.println("\n1.ENCUESTAS");
			System.out.println("\n2.ESTADISTICA");
			
	
		
			BufferedReader k = new BufferedReader(new InputStreamReader(System.in));
		
				System.out.print("\nESCOJA SU OPCION: ");
				opc = Integer.parseInt(k.readLine());
				
				encustados = null;
				switch (opc) {
				case 1:
				System.out.print("\nQUE DEPORTE PRACTICA?");
				
				
				System.out.println(" \n \na)TENIS");
				System.out.println("\nb)ATLETISMO");
				System.out.println("\nc)BALONCESTO  ");
				System.out.println("\nd)FUTBOL  ");
				System.out.println("\ne)AJEDREZ   ");
				System.out.println("\nf)NATACION ");
				System.out.println("\ng)VOLLEYBALL ");
				System.out.println("\nh)KARATE ");
				
				 encustados[0]="carlos";  
							encustados[1]="jose";
							encustados[2]="maria";
							encustados[3]="estela";
							encustados[4]="ana";
							encustados[5]="pedro";
							encustados[6]="rodrigo";
							encustados[7]="emilio";
							encustados[8]="ameth";
							encustados[9]="ricardo";
						   
							                    
									                   
				
				
					break;
				case 2:
					System.out.print("ANALISI DE LA ENCUESTA:");
					
				
					
				
				
					
					
					
					
					
					
					
					
					
					break;
					}

	}

	
}
	
				
					
				
					
					
					
					
					
							
				
					
					
					
					
					
					
					
					
					

				
				
			
					




	


					
						
					
					
	
					
					
				
	







			 		
		
		         
				 
				 			
					
					

							
				 
				  
				  
			
			
							
	




	


			
			
						
		
	


